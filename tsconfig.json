
{
  "compilerOptions": {
    "target": "es2020",
    "module": "commonjs",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "jsx": "react-jsx"
  },
  "include": [
    "api/**/*.ts",
    "**/*.tsx",
    "**/*.ts"
  ],
  "exclude": [
    "node_modules"
  ]
}
